--name:近三个月的收入前200的站点信息
--author:OrderPlus
--create time:2020-11-02 10:41
@gmv_info:=
select
  a.obj_id,
  sum(a.total_amount_all*a.currency_transform) as  total_amount_usd,
  sum((a.market_cost_cny - a.service_fees_cny)*a.currency_transform) as market_cost_usd
from app_looklook_st_financial_summary a
where a.pt = '${date}' and a.obj_type = 'site' and a.data_dt between '20210726' and '20210808'
group by a.obj_id
;
select
 a.erp_site_id,
 a.business_group_name,
 a.business_unit_name,
 b.total_amount_usd,
 b.market_cost_usd
from dwd_com_site_org_info a
left join @gmv_info b
on (a.site_id = b.obj_id)
