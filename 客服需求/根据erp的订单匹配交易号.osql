--name:根据erp的订单匹配交易号
--author:OrderPlus
--create time:2021-05-11 14:50
select b.order_no,b.id as erp_order_id,a.site_order_id,c.txn_id
from dwd_mapping_erp_order_id a
inner join ods_erp_erp_order_order b
on (a.erp_order_id = b.id and b.pt = '${date}')
left join dwd_mapping_txn_order_site c
on (a.site_order_id = c.order_id and c.pt = '${date}')
where a.pt = '${date}' and
